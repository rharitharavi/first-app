

<h1>Your Pragmatic Catalog</h1>
<% @products.each do |product| %>
<div class = "entry">
<h3><%= product.title %></h3>
<%= image_tag(product.image_url) %>
<%= sanitize(product.description) %>
<div class="price_line">
<span class="price">Cost : <%= product.price %></span>
<br/>
<br/>
</div>
</div>
<% end %> 
